#include <stdlib.h>
#include <time.h>
/* more headers goes there */
/* betty style doc for function main goes there */
int main(void)
{
int n;
srand(time(0));
n = rand() - RAND_MAX / 2;
/* your code goes there */
if(n >= 0);
{
/* I use else just to show an example */
printf("n is positive\n");
}
else
{
printf("n is negative\n");
/* Executed if no other statement is */
}
return (0);
}
